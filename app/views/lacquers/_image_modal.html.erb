<!-- Modal -->
<div class="modal fade" id="imageModal-<%= id %>" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog high">
    <div class="modal-content">
      <div class="modal-header" style="border-bottom: none; padding: 10px; margin: 0;">
        <a type="button" class="close" data-dismiss="modal" aria-label="Close">&times;</a>
      </div>
      <div class="modal-body text-center" style="padding-top: 0;">
        <%= image_tag image, width: '450px' %>
      </div>
    </div>
  </div>
</div>
