$('tbody').prepend('<%= j render :partial => "users/user_lacquer", :locals => {:user_lacquer => @user_lacquer, :user => @user, :favorite => @favorite} %>');

$('.add-lacquer.btn').blur();

$('#add-button').replaceWith('<%= j render :partial => "remove_button", :locals => {:user_lacquer => @user_lacquer} %>')

$('#edit_user_lacquer_wrapper').html('<%= j render partial: "lacquers/edit_user_lacquer", locals: { lacquer: @user_lacquer.lacquer } %>');

$('#new_review_link_wrapper').html('<%= j render partial: "lacquers/new_review", locals: {lacquer: @user_lacquer.lacquer} %>');
